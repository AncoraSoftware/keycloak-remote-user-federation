{
  "request": {
    "method": "GET",
    "urlPathEqualTo": "/api/users/find",
    "queryParameters": {
      "type": {
        "equalTo": "email"
      },
      "email": {
        "matches": ".*"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "id": "{{randomValue length=36 type='ALPHANUMERIC'}}",
      "firstName": "{{request.query.email.[0]}}",
      "lastName": "User",
      "userName": "{{request.query.email.[0]}}",
      "email": "{{request.query.email}}",
      "emailVerified": true,
      "enabled": true,
      "createdAt": "{{now format='yyyy-MM-dd HH:mm:ss'}}",
      "attributes": {
        "source": "email-lookup"
      },
      "roles": ["user"]
    }
  }
}
