{
  "request": {
    "method": "GET",
    "urlPathEqualTo": "/api/users/find",
    "queryParameters": {
      "type": {
        "equalTo": "username"
      },
      "username": {
        "equalTo": "nonexistent"
      }
    }
  },
  "response": {
    "status": 404,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "error": "User not found",
      "code": "USER_NOT_FOUND"
    }
  }
}
