{
  "request": {
    "method": "GET",
    "urlPathEqualTo": "/api/users/find",
    "queryParameters": {
      "type": {
        "equalTo": "id"
      },
      "id": {
        "matches": ".*"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "id": "{{request.query.id}}",
      "firstName": "Test",
      "lastName": "User",
      "userName": "testuser_{{request.query.id}}",
      "email": "testuser_{{request.query.id}}@example.com",
      "emailVerified": true,
      "enabled": true,
      "createdAt": "{{now format='yyyy-MM-dd HH:mm:ss'}}",
      "attributes": {
        "department": "QA",
        "location": "Office"
      },
      "roles": ["user", "tester"]
    }
  }
}
